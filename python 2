import turtle
my_screen=turtle.Screen()
my_screen.bgcolor("sky blue")
my_screen.title("turtle class")
my_turtle=turtle.Turtle()
for i in range(4):
    
        my_turtle.forward(50)
        my_turtle.right(90)
for i in range(3):
    for j in range(3):
        my_turtle.forward(50)
        my_turtle.right(120)
#inside the first for
        my_turtle.forward((50))
#outside the 'for'
        for i in range(3):
    for j in range(8):
        my_turtle.forward(100)
        my_turtle.backward(100)
        my_turtle.right(45)
